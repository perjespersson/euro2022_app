<div class="container" style="width: 1000px; margin-bottom: 20px;">
  <div class="row">
    <div class="col-12">
      <div class="col">
        <h1>EM 2022</h1>

          <ul class="nav nav-pills mb-3" id="pills-tab" role="tablist">
            <li class="nav-item" role="presentation">
              <button class="nav-link active" id="pills-home-tab" data-bs-toggle="pill" data-bs-target="#pills-home" type="button" role="tab" aria-controls="pills-home" aria-selected="true">RESULTAT</button>
            </li>
            <li class="nav-item" role="presentation">
              <button class="nav-link" id="pills-games-tab" data-bs-toggle="pill" data-bs-target="#pills-games" type="button" role="tab" aria-controls="pills-games" aria-selected="false">MATCHER</button>
            </li>
          </ul>

      </div>
    </div>
  </div>


  <div class="tab-content" id="pills-tabContent">
    <div class="tab-pane show active" id="pills-home" role="tabpanel" aria-labelledby="pills-home-tab">
      <%= render 'onepages/partials/statistics' %>
      <%= render 'onepages/partials/result' %>
      <%= render 'onepages/partials/recent_and_upcoming_games' %>
    </div>

    <div class="tab-pane" id="pills-games" role="tabpanel" aria-labelledby="pills-games-tab">
      <%= render 'onepages/partials/games' %>
    </div>
  </div>
</div>

